                android:textColor="@color/mainColor4"
                android:textSize="18sp"
                android:layout_marginTop="20dp" />
            <TextView
                android:id="@+id/carotene_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="carotene"
                android:layout_alignParentRight="true"
                android:layout_marginRight="50dp"
                android:layout_alignTop="@+id/carotene_label"
                android:textSize="18sp"
                android:textColor="@color/mainColor" />
            <!--硫胺-->
            <TextView
                android:id="@+id/liuan_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="硫胺："
                android:layout_below="@id/carotene_text"
                android:layout_alignLeft="@id/carotene_label"
                android:textColor="@color/mainColor4"
                android:textSize="18sp"
                android:layout_marginTop="20dp" />
            <TextView
                android:id="@+id/liuan_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="liuan"
                android:layout_alignParentRight="true"
                android:layout_marginRight="50dp"
                android:layout_alignTop="@+id/liuan_label"
                android:textSize="18sp"
                android:textColor="@color/mainColor" />
            <!--核黄素-->
            <TextView
                android:id="@+id/hehuang_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="核黄素："
                android:layout_below="@id/liuan_text"
                android:layout_alignLeft="@id/liuan_label"
                android:textColor="@color/mainColor4"
