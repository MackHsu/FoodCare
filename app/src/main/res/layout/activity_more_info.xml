                android:layout_alignTop="@+id/tie_label"
                android:textSize="18sp"
                android:textColor="@color/mainColor" />
            <!--锌-->
            <TextView
                android:id="@+id/xin_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="锌："
                android:layout_below="@id/tie_text"
                android:layout_alignLeft="@id/tie_label"
