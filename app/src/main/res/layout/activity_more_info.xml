                android:layout_marginTop="20dp" />
            <TextView
                android:id="@+id/meng_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="meng"
                android:layout_alignParentRight="true"
                android:layout_marginRight="50dp"
                android:layout_alignTop="@+id/meng_label"
                android:textSize="18sp"
                android:textColor="@color/mainColor" />
            <!--钾-->
            <TextView
                android:id="@+id/jia_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="钾："
                android:layout_below="@id/meng_text"
                android:layout_alignLeft="@id/meng_label"
                android:textColor="@color/mainColor4"
                android:textSize="18sp"
                android:layout_marginTop="20dp" />
            <TextView
                android:id="@+id/jia_text"
                android:layout_width="wrap_content"
