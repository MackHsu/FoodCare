<?xml version="1.0" encoding="utf-8"?>
<!--许朗铭-->
<!--食物详细 界面-->

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!--标题栏布局容器-->
    <RelativeLayout
        android:id="@+id/title_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/mainColor1">
        <!--返回按钮-->
        <ImageButton
            android:id="@+id/back_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/back_button"
            android:background="#00000000"
            android:layout_margin="10dp" />
        <!--标题 名称-->
        <TextView
            android:id="@+id/title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="营养元素"
            android:textColor="#fff"
            android:textSize="20sp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/title_layout">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.victor.loading.rotate.RotateLoading
                android:id="@+id/loading"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                app:loading_color="@color/mainColor1"
                app:loading_speed="11"
                app:loading_width="2dp"
                android:layout_margin="10dp"
                android:layout_centerHorizontal="true"/>
        </RelativeLayout>
    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <!--名称-->
            <TextView
                android:id="@+id/food_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="FoodName"
                android:textSize="28sp"
                android:textColor="@color/mainColor4"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:textStyle="bold"/>
            <!--纤维素-->
            <TextView
                android:id="@+id/cellulose_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="纤维素："
                android:layout_below="@id/food_name"
                android:layout_alignLeft="@id/food_name"
                android:textColor="@color/mainColor4"
                android:textSize="18sp" />
            <TextView
                android:id="@+id/cellulose_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="cellulose"
                android:layout_alignParentRight="true"
                android:layout_marginRight="50dp"
                android:layout_alignTop="@+id/cellulose_label"
                android:textSize="18sp"
                android:textColor="@color/mainColor" />
            <!--维他命A-->
            <TextView
                android:id="@+id/vA_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="维他命A："
                android:layout_below="@id/cellulose_text"
                android:layout_alignLeft="@id/cellulose_label"
                android:textColor="@color/mainColor4"
                android:textSize="18sp"
                android:layout_marginTop="20dp" />
            <TextView
                android:id="@+id/vA_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="vA"
                android:layout_alignParentRight="true"
                android:layout_marginRight="50dp"
                android:layout_alignTop="@+id/vA_label"
                android:textSize="18sp"
                android:textColor="@color/mainColor" />
            <!--维他命C-->
            <TextView
                android:id="@+id/vC_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="维他命C："
                android:layout_below="@id/vA_text"
                android:layout_alignLeft="@id/vA_label"
                android:textColor="@color/mainColor4"
                android:textSize="18sp"
                android:layout_marginTop="20dp" />
            <TextView
