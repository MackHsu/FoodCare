                android:layout_alignTop="@+id/vC_label"
                android:textSize="18sp"
                android:textColor="@color/mainColor" />
            <!--维他命E-->
            <TextView
                android:id="@+id/vE_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
